{
    "title": "Simultaneous chords",
    "rules": [{
        "description": "ef -> app_expose",
        "manipulators": [{
            "type": "basic",
            "from": {
                "simultaneous": [{
                        "key_code": "e"
                    },
                    {
                        "key_code": "f"
                    }
                ],
                "modifiers": {
                    "optional": [
                        "any"
                    ]
                }
            },
            "to": [{
                "key_code": "mission_control",
                "modifiers": [
                    "left_control"
                ]
            }]
        }]
    }, {
        "description": "wf -> mission_control",
        "manipulators": [{
            "type": "basic",
            "from": {
                "simultaneous": [{
                        "key_code": "w"
                    },
                    {
                        "key_code": "f"
                    }
                ],
                "modifiers": {
                    "optional": [
                        "any"
                    ]
                }
            },
            "to": [{
                "key_code": "mission_control"
            }]
        }]
    }, {
        "description": "qf -> show_desktop",
        "manipulators": [{
            "type": "basic",
            "from": {
                "simultaneous": [{
                        "key_code": "q"
                    },
                    {
                        "key_code": "f"
                    }
                ],
                "modifiers": {
                    "optional": [
                        "any"
                    ]
                }
            },
            "to": [{
                "key_code": "mission_control",
                "modifiers": [
                    "left_command"
                ]
            }]
        }]
    }, {
        "description": "jk -> lang_switch",
        "manipulators": [{
            "type": "basic",
            "from": {
                "simultaneous": [{
                        "key_code": "j"
                    },
                    {
                        "key_code": "k"
                    }
                ],
                "modifiers": {
                    "optional": [
                        "any"
                    ]
                }
            },
            "to": [{
                "key_code": "spacebar",
                "modifiers": [
                    "left_command"
                ]
            }]
        }]
    }]
}
